{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "inputs": ["$TURBO_DEFAULT$", ".env*"],
            "outputs": [
                "dist/**",
                ".next/**",
                "!.next/cache/**",
                "./open-next/**",
                "!.open-next/cache/**"
            ]
        },
        "lint": {},
        "type-check": {},
        "dev": {
            "cache": false,
            "persistent": true
        },
        "clean": {
            "cache": false
        }
    }
}
